   "source": [
    "def roots(a, b, c):\n",
    "    disc = b**2 - 4 * a * c\n",
    "    if(disc < 0):\n",
    "        print(\"Root is complex: \", end='')\n",
    "    return (-b + disc**0.5) / (2 * a), (-b - disc**0.5) / (2 * a)"
   ]
  },
